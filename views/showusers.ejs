<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Showusers</title>
    <style>
       table, thead,tr,th,td,tbody{
            border: solid 1px black;
        }
    </style>
</head>
<body>
    <h1>This is faker table</h1>
   <table>
    <thead>
        <tr>
            <th>id</th>
            <th>username</th>
            <th>email</th>
            <th>password</th>
        </tr>
    </thead>

    <tbody>
       <% for(let users of user){ %>
    <tr>
        <td> <%=users.id %> </td>
        <td> <%=users.username %> </td>
        <td> <%=users.email %> </td>
        <td> <%=users.password %> </td>
        <td>
            <form method="GET" action="/user/<%= users.id %>/edit">
            <button>Edit username</button>
        </form>
        </td>
        
   </tr> 
<% } %>
    </tbody>
</table>
</body>
</html>

